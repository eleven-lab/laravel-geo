{
  "name": "karomap/laravel-geo",
  "description": "Spatial OGC objects integration for Laravel 5.*",
  "license": "GPL-3.0-only",
  "keywords": [
    "geospatial",
    "laravel",
    "spatial",
    "geo"
  ],
  "authors": [
    {
      "name": "Lorenzo Giustozzi",
      "email": "giustozzi@lorenzo.im"
    },
    {
      "name": "Rifa'i M. Hanif",
      "email": "hanreev@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6.4",
    "illuminate/database": "^5.4",
    "karomap/php-ogc": "@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "laravel/framework": "^5.4",
    "doctrine/dbal": "^2.9",
    "fzaninotto/faker": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "Karomap\\GeoLaravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "TestApp\\": "tests/app/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Karomap\\GeoLaravel\\DatabaseServiceProvider"
      ],
      "aliases": {
        "GeoModel": "Karomap\\GeoLaravel\\Eloquent\\Model"
      }
    }
  },
  "prefer-stable": true,
  "repositories": [
    {
      "type": "path",
      "url": "../php-ogc"
    }
  ]
}
